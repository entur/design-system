{
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test"],
        "accessToken": "OTE1MzRlMzEtZmUyNi00MmY1LThkNTktODBkNGI4ZmZlMDBhfHJlYWQ="
      }
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "!{projectRoot}/*.md"],
    "production": ["{projectRoot}/dist"],
    "configs": [
      "{workspaceRoot}/dts.config.js",
      "{workspaceRoot}/tsconfig*.json",
      "{workspaceRoot}/package.json",
      "{workspaceRoot}/.eslintrc"
    ]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["default", "configs", "^production"],
      "outputs": ["{projectRoot}/dist"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "inputs": ["default", "configs"]
    }
  }
}
