{
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test"],
        "accessToken": "OTE1MzRlMzEtZmUyNi00MmY1LThkNTktODBkNGI4ZmZlMDBhfHJlYWQ="
      }
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*"]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "default",
        "{workspaceRoot}/dts.config.js",
        "{workspaceRoot}/tsconfig*.json",
        "{workspaceRoot}/package.json",
        "!{projectRoot}/*.md"
      ],
      "outputs": ["{projectRoot}/dist"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc"]
    }
  }
}
